{
    "name": "onigoetz/deployer",
    "description": "A configurable deploy script",
    "homepage": "https://github.com/onigoetz/deployer",
    "keywords": ["deployment", "laravel", "l4", "capistrano"],
    "authors": [
        {
            "name": "Stephane Goetz",
            "email": "onigoetz@onigoetz.ch"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/console": "~2.2|~3.0",
        "phpseclib/phpseclib": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0",
        "squizlabs/php_codesniffer": "~2.0",
        "phpmd/phpmd": "~2.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Onigoetz\\Deployer\\": "src/"
        }
    },
    "bin": ["src/bin/deployer"]
